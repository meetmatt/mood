{
  "name": "meetmatt/colla-mood",
  "description": "Colla.io - Mood",
  "license": "proprietary",
  "minimum-stability": "stable",
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "MeetMatt\\Colla\\Mood\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MeetMatt\\Colla\\Mood\\Test\\": "test/"
    }
  },
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "ext-pdo": "*",
    "paragonie/easydb": "^2.8",
    "ramsey/uuid": "^3.8",
    "slim/slim": "^3.12",
    "slim/twig-view": "^2.4",
    "symfony/console": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2"
  }
}
